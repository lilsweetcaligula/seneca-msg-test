{
  "name": "seneca-msg-test",
  "version": "1.2.0",
  "description": "Structured testing of seneca plugin messages.",
  "main": "msg-test.js",
  "scripts": {
    "test": "lab -P test -t 80 -I URL,URLSearchParams,BigUint64Array,BigInt64Array,BigInt",
    "prettier": "prettier --write --no-semi --single-quote *.js lib/*.js test/*.js",
    "coveralls": "lab -v -P test -r lcov | coveralls",
    "clean": "rm -rf node_modules yarn.lock package-lock.json",
    "repo-tag": "REPO_VERSION=`node -e \"console.log(require('./package').version)\"`; echo TAG: v$REPO_VERSION && git commit -a -m v$REPO_VERSION && git push && git tag v$REPO_VERSION && git push --tags;",
    "repo-publish": "npm run prettier && npm test && npm run repo-tag && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voxgig/seneca-msg-test.git"
  },
  "keywords": [
    "seneca",
    "msg",
    "test"
  ],
  "author": "Richard Rodger richardrodger.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/voxgig/seneca-msg-test/issues"
  },
  "homepage": "https://github.com/voxgig/seneca-msg-test#readme",
  "dependencies": {
    "inks": "^0.1.0",
    "jsonic": "^0.3.1",
    "optioner": "^4.0.0"
  },
  "devDependencies": {
    "code": "^5.2.4",
    "coveralls": "^3.0.3",
    "lab": "^18.0.2",
    "prettier": "^1.17.1",
    "seneca": "^3.11.0",
    "seneca-entity": "^4.0.0",
    "seneca-promisify": "^0.8.0"
  }
}
